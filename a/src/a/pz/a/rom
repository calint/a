{
@inline print_bmp_4x4(0x0800 4 4 252 0xfafa 0xf00)
@inline print_bmp_4x4(0x0804 4 4 252 0xfafa 0xf00)

def print_bmp_4x4(scr wi hi ln bmp rgb){
  lp(hi){
    lp(wi){
      shf(bmp -1)
      @ifn stc(scr rgb)
      @ifz inc(scr)
      @ifp inc(scr)
    }
    add(scr ln)
  }
}

def red 0xf00
def green 0xf0
def blue 0xf
def eof 0xffff
}