{
line(screen red 0x30)
rect(screen red 0x30 0x40 0x20)
dot(screen red)
dot(screen2 blue)

eof
def dots{
  0x1040 0x800
  0x1050 0x080
  0x1060 0x008
}
def dot(a b){st(a b)}

eof
def line(a b c){
  lp(c){
    stc(a b)
    inc(b)
  }
}

eof
def rect(a b c d e){
  lp(d){
    lp(c){
      stc(a b)
      inc(b)
    }
    add(a e)
  }
}
eof

def screen 0x1000
def red 0xf00
def screen2 0x4060
def blue 0xf
def eof 0xffff
}