{
line(screen red 0x30)
rect(screen red 0x30 0x40 0x20)
dots.draw()
eof

def dots{
  3
  0x4040 0x800
  0x4050 0x080
  0x4060 0x008
  0x0000 0x000
}
def dots.draw(){
  li(a dots)
  ldc(c a)
  lp(c){
    ldc(d a)
    ldc(e a)
    st(d e)
  }
}


def dot(a b){st(a b)}

eof
def line(a b c){
  lp(c){
    stc(a b)
    inc(b)
  }
}

eof
def rect(a b c d e){
  lp(d){
    lp(c){
      stc(a b)
      inc(b)
    }
    add(a e)
  }
}
eof

def screen 0x1000
def red 0xf00
def screen2 0x4060
def blue 0xf
def eof 0xffff
}